<!DOCTYPE html>
<html>
  <!-- HEAD -->
  <%- include('./partials/head') %>

  <body>
    <!-- HEADER -->
    <%- include('./partials/header') %>

    <!-- MAIN -->
    <main>
      <!--h1>Formulario actualización de datos</h1>
      <hr size="" color="black"-->

      <form action='/post' method='post' id='formNewDataMembers' name='formNewDataMembers' enctype='multipart/form-data' class='formNewDataMembers'>

<!------------------------------ SECTION 0 WELCOME ------------------------------>
        <section class='welcome' id='welcome'>

          <!--ADELANTE/ATRÁS-->
          <div class='btnBox'>
            <div class='btn' id='btnWelcomeBack'>
              <h3>ATRÁS</h3>
            </div>
            <div class='btn' id='btnWelcomeContinue'>
              <h3>CONTINUAR</h3>
            </div>
          </div>

        </section>
<!------------------------------ SECTION 1 PERSONAL ------------------------------>
        <!--CONTENIDO MODAL PERSONAL-->
        <div class="modalPersonal">
          <div class="contenedorPersonal">
            <article class="header"><h2 class="encabezadoPersonal">Personal</h2></article>
            <div class="contenidoPersonal">
              
              <!--SECTION PERSONAL-->
              <section class='personal' id='personal'>

                <h2>Personal</h2>

                <!--NOMBRE-->
                <div class='text'>
                  <input type='text' name='firstName' id='firstName' class= 'input-text' placeholder='Nombre'  maxlength='50'>
                </div>

                <!--APELLIDO-->
                <div class='text'>
                  <input type='text' name='lastName' id='lastName' class= 'input-text' placeholder='Apellido'   maxlength='50'>
                </div>

                <!--NACIMIENTO-->
                <div class='date'>
                  <div class='paragrph'>
                    <p>Fecha de nacimiento</p>
                  </div>

                  <div class='dateLabel'>
                    <input type='date' name='dateOfBirthAt' id='dateOfBirthAt' class= 'input-date' >
                  </div>
                </div>

                <!--GENERO-->
                <div class='mark'>
                  <div class='paragrph'>
                    <p>Genero:</p>
                  </div>

                  <div class='insideLabelRow'>
                    <div class="inputRow">
                      <div class="radiobutton"><input type='radio' name='sex' value='hombre' id='sexHombre' ></div>
                      <div class="checkLabelrRow"><p><strong>H</strong></p></div> 
                    </div>

                    <div class="inputRow">
                      <div class="radiobutton"><input type='radio' name='sex' value='mujer' id='sexMujer' ></div>
                      <div class="checkLabelRow"><p><strong>M</strong></p></div>
                    </div>
                    
                    <div class="inputRow">
                      <div class="radiobutton"><input type='radio' name='sex' value='noContesta' id='sexNoContesta' ></div>
                      <div class="checkLabelRow"><p><strong>N/C</strong></p></div>
                    </div>
                  </div>
                </div>

                <!--SITUACION RELACIONAL-->
                <div class='mark'>
                  <div class='paragrph'>
                    <p>Situación relacional:</p>
                  </div>

                  <div class='insideLabelColumn'>
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='radio' name='relationalSituation' value='soltero' id='relationalSituation-s' ></div>
                      <div class="checkLabel"><p><strong>Soltero/a</strong></p></div>  
                    </div>
                    
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='radio' name='relationalSituation' value='enPareja' id='relationalSituation-ep' ></div>
                      <div class="checkLabel"><p><strong>En pareja</strong></p></div>  
                    </div>

                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='radio' name='relationalSituation' value='casado' id='relationalSituation-c' ></div>
                      <div class="checkLabel"><p><strong>Casado/a</strong></p></div>  
                    </div>

                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='radio' name='relationalSituation' value='viudo' id='relationalSituation-v' ></div>
                      <div class="checkLabel"><p><strong>Viudo/a</strong></p></div>  
                    </div>

                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='radio' name='relationalSituation' value='divorciado' id='relationalSituation-d' ></div>
                      <div class="checkLabel"><p><strong>Divorciado/a</strong></p></div>  
                    </div>

                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='radio' name='relationalSituation' value='otro' id='relationalSituation-o' ></div>
                      <div class="checkLabel"><p><strong>Otra</strong></p></div>
                    </div>
                  </div>
                </div>

                <!--ADELANTE/ATRÁS-->
                <div class='btnBox'>
                  <div class='btn' id='btnPersonalBack'>
                    <h3>ATRÁS</h3>
                  </div>

                  <div class='btn' id='btnPersonalContinue'>
                    <h3>CONTINUAR</h3>
                  </div>
                </div>


              </section>
      
            </div>
          </div>
        </div>

<!------------------------------ SECTION 2 FAMILY ------------------------------>
        <!--CONTENIDO MODAL FAMILY-->
        <div class="modalFamily">
          <div class="contenedorFamily">
            <article class="header"><h2 class="encabezadoFamily">Family</h2></article>
            <div class="contenidoFamily">
              
              <!--SECTION-->
              <section class='family' id='family'>

                <h2>Familia</h2>
              
                <!--CONVIVENCIA-->
                <div class='mark'>
                  <div class='paragrph'>
                    <p>¿Con quien vives?</p>
                  </div>
              
                  <div class='insideLabelColumn'>
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='coexistenceSituation' value='p' id='coexistenceSituation-p' ></div>
                      <div class="checkLabel"><p><strong>Papá</strong></p></div>
                    </div>
              
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='coexistenceSituation' value='m' id='coexistenceSituation-m' ></div>
                      <div class="checkLabel"><p><strong>Mamá</strong></p></div>
                    </div>
              
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='coexistenceSituation' value='t' id='coexistenceSituation-t' ></div>
                      <div class="checkLabel"><p><strong>Tío/a</strong></p></div>
                    </div>
              
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='coexistenceSituation' value='a' id='coexistenceSituation-a' ></div>
                      <div class="checkLabel"><p><strong>Abuelo/a </strong></p></div>
                    </div>
              
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='coexistenceSituation' value='c' id='coexistenceSituation-c' ></div>
                      <div class="checkLabel"><p><strong>Cónyuge</strong></p></div>
                    </div>
              
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='coexistenceSituation' value='h' id='coexistenceSituation-h' ></div>
                      <div class="checkLabel"><p><strong>Hijo/s</strong></p></div>
                    </div>
              
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='coexistenceSituation' value='s' id='coexistenceSituation-s' ></div>
                      <div class="checkLabel"><p><strong>Solo/a</strong></p></div>
                    </div>
              
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='coexistenceSituation' value='o' id='coexistenceSituation-s' ></div>
                      <div class="checkLabel"><p><strong>Otro</strong></p></div>
                    </div>
                  </div>
                </div>
              
                <!--HIJOS-->
                <div class='mark'>
                  <div class='paragrph'>
                    <p>¿Tiene hijos?</p>
                  </div>
              
                  <div class='insideLabelRow'>
                    <div class="inputRow">
                      <div class="radiobutton"><input type='radio' name='isChildren' value='s' id='isChildren-s' ></div>
                      <div class="checkLabelRow"><p><strong>Sí</strong></p></div>
                    </div>
              
                    <div class="inputRow">
                      <div class="radiobutton"><input type='radio' name='isChildren' value='n' id='isChildren-n'  checked></div>
                      <div class="checkLabelRow"><p><strong>No</strong></p></div>
                    </div>
                  </div>
              
                  <div class='text'><input type='number' name='numberChildren' value='' id='numberChildren-n' placeholder="¿Cuantos?"></div>
                  <div class='text'><input type='number' name='isChildrenAttendChurch' value='' id='isChildrenAttendChurch-n' placeholder="¿Cuantos asisten a la iglesia?"></div>
                </div>
              
                <!--ADELANTE/ATRÁS-->
                <div class='btnBox'>
                  <label for="btn-modalFamily">
                    <div class='btn' id='btnFamilyBack'>
                      <h3>ATRÁS</h3>
                    </div>
                  </label>

                  <label for="btn-modalResidency">
                    <div class='btn' id='btnFamilyContinue'>
                      <h3>CONTINUAR</h3>
                    </div>
                  </label>
                </div>
              
              </section>
              
            </div>
          </div>
        </div>  

<!------------------------------ SECTION 3 RESIDENCY ------------------------------>
        <!--CONTENIDO MODAL RESIDENCY-->
        <div class="modalResidency">
          <div class="contenedorResidency">
            <article class="header"><h2 class="encabezadoResidency">Residency</h2></article>
            <div class="contenidoResidency">
              
              <!--SECTION-->
              <section class='residency' id='residency'>

                <h2>Recidencia</h2>
      
                <!--CALLE-->
                <div class='text'>
                  <input type='text' name='street' id='street' class= 'input-text' placeholder='Calle'  maxlength='50'>
                </div>
      
                <!--NUEMRACION-->
                <div class='text'>
                  <input type='text' name='numeration' id='numeration' class= 'input-text' placeholder='Numeración'  maxlength='50'>
                </div>
                    
                <!--ACLARACION-->
                <div class='text'>
                  <input type='text' name='secondLine' id='secondLine' class= 'input-text' placeholder='A tener en cuenta'  maxlength='200'>
                </div>
      
                <!--BARRIO-->
                <div class='text'>                  
                  <input type='text' name='neighborhood' id='neighborhood' class= 'input-text' placeholder='Barrio'  maxlength='50'>
                </div>
      
                <!--MANZANA-->
                <div class='text'>                  
                  <input type='text' name='neighborhoodBlock' id='neighborhoodBlock' class= 'input-text' placeholder='Manzana' maxlength='50'>
                </div>
      
                <!--CASA-->
                <div class='text'>                  
                  <input type='text' name='houseNumber' id='houseNumber' class= 'input-text' placeholder='Casa' maxlength='50'>
                </div>
      
                <!--CODIGO POSTAL-->
                <div class='text'>
                  <input type='text' name='postalCode' id='postalCode' class= 'input-text' placeholder='Código postal'  maxlength='50'>
                </div>
                    
                <!--LOCALIDAD-->
                <div class='text'>
                  <input type='text' name='location' id='location' class= 'input-text' placeholder='Localidad' maxlength='50'>
                </div>
                    
                <!--DEPARTAMENTO-->
                <div class='text'>
                  <input type='text' name='city' id='city' class= 'input-text' placeholder='Departamento'  maxlength='50'>
                </div>
                    
                <!--PROVINCIA-->
                <div class='text'>
                  <input type='text' name='state' id='state' class= 'input-text' placeholder='Provincia'  maxlength='50'>
                </div>
                    
                <!--PAIS-->
                <div class='text'>
                  <input type='text' name='country' id='country' class= 'input-text' placeholder='País'  maxlength='50'>
                </div>
      
                <!--ADELANTE/ATRÁS-->
                <div class='btnBox'>
                  <div class='btn' id='btnResidencyBack'>
                    <h3>ATRÁS</h3>
                  </div>

                  <div class='btn' id='btnResidencyContinue'>
                    <h3>CONTINUAR</h3>
                  </div>
                </div>

              </section>
              
            </div>
          </div>
        </div>
        
<!------------------------------ SECTION 4 CONTACT ------------------------------>
        <!--CONTENIDO MODAL CONTACT-->
        <div class="modalContact">
          <div class="contenedorContact">
            <article class="header"><h2 class="encabezadoContact">Contact</h2></article>
            <div class="contenidoContact">
              
              <!--SECTION-->
              <section class='contact' id='contact'>

                <h2>Contacto</h2>
      
                <!--CELULAR-->
                <div class='text'>
                  <input type='number' name='valueContact' id='valueContact' class= 'input-text' placeholder='Telefono celular'  maxlength='50'>
                </div>
      
                <!--FIJO-->
                <div class='text'>
                  <input type="number" name='fijo' id='fijo' class= 'input-text' placeholder='Fijo (opcional)' maxlength='50'>
                </div>
      
                <!--CORREO-->
                <div class='text'>
                  <input type="email" name="email" id="email" class= "input-text"  placeholder='Email'  minlength="5" maxlength="50">
                </div>
      
                <!--REDES-->
                <div class='mark'>
                  <div class='paragrph'>
                    <p>Redes sociales</p>
                  </div>
      
                  <div class='insideLabelColumn'>
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='YouVersion' id='webSite-YouVersion' ></div>
                      <div class='checkLabel'><p><strong>YouVersion</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='Facebook' id='webSite-Facebook' ></div>
                      <div class='checkLabel'><p><strong>Facebook</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='Instagram' id='webSite-Instagram' ></div>
                      <div class='checkLabel'><p><strong>Instagram</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='YouTube' id='webSite-YouTube' ></div>
                      <div class='checkLabel'><p><strong>YouTube</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='Tik' id='webSite-Tik' ></div>
                      <div class='checkLabel'><p><strong>Tik Tok</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='Spotify' id='webSite-Spotify' ></div>
                      <div class='checkLabel'><p><strong>Spotify</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='WhatsApp' id='webSite-WhatsApp' ></div>
                      <div class='checkLabel'><p><strong>WhatsApp</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='Telegram' id='webSite-Telegram' ></div>
                      <div class='checkLabel'><p><strong>Telegram</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='Twitter' id='webSite-Twitter' ></div>
                      <div class='checkLabel'><p><strong>Twitter</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='Twitch' id='webSite-Twitch' ></div>
                      <div class='checkLabel'><p><strong>Twitch</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='Discord' id='webSite-Discord' ></div>
                      <div class='checkLabel'><p><strong>Discord</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='Pinterest' id='webSite-Pinterest' ></div>
                      <div class='checkLabel'><p><strong>Pinterest</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='LinkedIn' id='webSite-LinkedIn' ></div>
                      <div class='checkLabel'><p><strong>LinkedIn</strong></p></div> 
                    </div>
      
                    <div class='inputColumn'>
                      <div class="radiobutton"><input type='checkbox' name='webSite' value='Slack' id='webSite-Slack' ></div>
                      <div class='checkLabel'><p><strong>Slack</strong></p></div> 
                    </div>    
                  </div>
                </div>
      
                <!--ADELANTE/ATRÁS-->
                <div class='btnBox'>
                  <div class='btn' id='btnContactBack'>
                    <h3>ATRÁS</h3>
                  </div>
                  <div class='btn' id='btnContactContinue'>
                    <h3>CONTINUAR</h3>
                  </div>
                </div>

              </section>
              
            </div>
          </div>
        </div>

<!------------------------------ SECTION 5 GROWNUP ------------------------------>
        <!--CONTENIDO MODAL GROWNUP-->
        <div class="modalGrownup">
          <div class="contenedorGrownup">
            <article class="header"><h2 class="encabezadoGrownup">Grownup</h2></article>
            <div class="contenidoGrownup">
              
              <!--SECTION-->
              <section class='grownup' id='grownup'>

                <h2>Crecimiento Cristiano</h2>
      
                <!--TIEMPO DE ASISTENCIA-->
                <div class='mark'>
                  <div class='paragrph'>
                    <p>¿Desde cuándo asistís a nuestra iglesia?</p>
                  </div>
      
                  <div class='insideLabelColumn'>
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='radio' name='timeChurch' value='timeChurch-0' id='timeChurch-0' ></div>
                      <div class="checkLabel"><p><strong>Menos de 6 meses</strong></p></div>
                    </div>
          
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='radio' name='timeChurch' value='timeChurch-6-1' id='timeChurch-6-1' ></div>
                      <div class="checkLabel"><p><strong>Entre 6 meses a 1 año</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='radio' name='timeChurch' value='timeChurch-1-5' id='timeChurch-1-5' ></div>
                      <div class="checkLabel"><p><strong>Entre 1 año a 5 años</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='radio' name='timeChurch' value='timeChurch-5-0' id='timeChurch-5-0' ></div>
                      <div class="checkLabel"><p><strong>Más de 5 años</strong></p></div>
                    </div>
                  </div>
                </div>
                      
                <!--EXPERIENCIAS COMPLETADAS-->
                <div class='mark'>
                  <div class='paragrph'>
                    <p>Marca las experiencias completadas</p>
                  </div>
      
                  <div class='insideLabelColumn'>
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='nuevoInicio' id='churchExperiences-nuevoInicio' ></div>
                      <div class="checkLabel short"><p><strong>Nuevo Inicio</strong></p></div>
                      <div class="checkLabel longer"><p><strong>Nuevo Inicio con Dios</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='bautismo' id='churchExperiences-bautismo' ></div>
                      <div class="checkLabel"><p><strong>Bautismo</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='p1' id='churchExperiences-p1' ></div>
                      <div class="checkLabel short"><p><strong>Paso 1-Miembro</strong></p></div>
                      <div class="checkLabel longer"><p><strong>Paso 1-Charla de nuevos Miembros</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='p2' id='churchExperiences-p2' ></div>
                      <div class="checkLabel short"><p><strong>Paso 2-Forma</strong></p></div>
                      <div class="checkLabel longer"><p><strong>Paso 2-Descubre tu Forma</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='p3' id='churchExperiences-p3' ></div>
                      <div class="checkLabel short"><p><strong>Paso 3-Madrez</strong></p></div>
                      <div class="checkLabel longer"><p><strong>Paso 3-Adopta Hábitos de Madurez</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='p4' id='churchExperiences-p4' ></div>
                      <div class="checkLabel short"><p><strong>Paso 4-Voluntario</strong></p></div>
                      <div class="checkLabel longer"><p><strong>Paso 4-Voluntariado inicial</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='vd1' id='churchExperiences-vd1' ></div>
                      <div class="checkLabel short"><p><strong>Vida 1</strong></p></div>
                      <div class="checkLabel longer"><p><strong>Vida Discipular 1</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='vd2' id='churchExperiences-vd2' ></div>
                      <div class="checkLabel short"><p><strong>Vida 2</strong></p></div>
                      <div class="checkLabel longer"><p><strong>Vida Discipular 2</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='vd3' id='churchExperiences-vd3' ></div>
                      <div class="checkLabel short"><p><strong>Vida 3</strong></p></div>
                      <div class="checkLabel longer"><p><strong>Vida Discipular 3</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='vd4' id='churchExperiences-vd4' ></div>
                      <div class="checkLabel short"><p><strong>Vida 4</strong></p></div>
                      <div class="checkLabel longer"><p><strong>Vida Discipular 4</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='libre' id='churchExperiences-libre' ></div>
                      <div class="checkLabel"><p><strong>Sea Libre</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='fundamentos' id='churchExperiences-fundamentos' ></div>
                      <div class="checkLabel"><p><strong>Fundamentos</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='celebremos' id='churchExperiences-celebremos' ></div>
                      <div class="checkLabel short"><p><strong>Celebremos</strong></p></div>
                      <div class="checkLabel longer"><p><strong>Celebremos la Recuperación</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='churchExperiences' value='otro' id='churchExperiences-otro' ></div>
                      <div class="checkLabel"><p><strong>Otros</strong></p></div>
                    </div>
                  </div>
                </div>
                               
                <!--SERVICIOS USADOS-->
