                  <div class='paragrph'>
                    <p>Servicios que has utilizados</p>
                  </div>
      
                  <div class='insideLabelColumn'>
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='servicesUsed' value='cafeteria' id='servicesUsed-cafeteria' ></div>
                      <div class="checkLabel"><p><strong>Cafetería</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='servicesUsed' value='camping' id='servicesUsed-camping' ></div>
                      <div class="checkLabel"><p><strong>Camping</strong></p></div>
                    </div>
                    
                    <div class="inputColumn">
                      <div class="radiobutton"><input type='checkbox' name='servicesUsed' value='escuela-musica' id='servicesUsed-escuela-musica' ></div>
                      <div class="checkLabel"><p><strong>Escuela de Música</strong></p></div>
